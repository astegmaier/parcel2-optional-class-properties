{
  "name": "parcel2-optional-class-properties",
  "version": "1.0.0",
  "private": true,
  "browser": "dist/index.html",
  "repository": "https://github.com/astegmaier/parcel2-optional-class-properties.git",
  "author": "Andrew Stegmaier <ansteg@microsoft.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && parcel build src/index.html",
    "start": "parcel serve src/index.html"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "parcel": "^2.0.0-nightly.83",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-typescript",
        {
          "isTsx": true
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx",
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
